        "css-loader": "^3.2.0",
        "laravel-mix": "^4.0.7",
        "lodash": "^4.17.19",
        "node-sass": "^4.13.0",
        "path": "^0.12.7",
        "resolve-url-loader": "^2.3.1",
        "sass": "^1.15.2",
        "sass-loader": "^7.3.1",
        "vue-loader": "^15.7.2",
        "vue-resource": "^1.5.1",
        "vue-router": "^3.1.3",
        "vue-style-loader": "^4.1.2",
        "vue-template-compiler": "^2.6.10",
        "webpack": "^4.41.2",
        "webpack-cli": "^3.3.10",
        "webpack-dev-server": "^3.9.0"
